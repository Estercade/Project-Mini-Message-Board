<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <%- include('navbar', {links: links})  %>
        <ul>
            <% messages.forEach((message) => { %>
                <li>
                    <p><%= message.text %></p>
                    <p><%= message.user %></p>
                    <p><%= message.added %></p>
                    <a href="/messages/<%= message.id %>">View message</a>
                </li>
            <% }); %>
        </ul>   
    </body>
</html>